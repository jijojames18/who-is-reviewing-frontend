{
  "manifest_version": 3,
  "name": "Who is reviewing",
  "version": "1.0",
  "description": "Browser extension to show the list of users actively reviewing a pull request.",
  "author": "Jijo James",
  "background": {
    "service_worker": "script.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.github.com/*/pull/*"],
      "js": ["config.js", "populate.js"],
      "run_at": "document_start"
    }
  ]
}
